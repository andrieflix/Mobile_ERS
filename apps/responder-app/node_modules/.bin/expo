#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules/expo/bin/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules/expo/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules/expo/bin/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules/expo/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/expo@49.0.23_@babel+core@7.26.10/node_modules:/mnt/c/Users/IC2/Desktop/Netlify Project Sample/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
